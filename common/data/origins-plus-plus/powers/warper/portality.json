{
	"type":"origins:multiple",
	"toggled_blue":{
		"type":"origins:resource",
		"min":0,
		"max":1,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_01.png",
			"bar_index":20,
			"condition":{
				"type":"origins:resource",
				"comparison":"==",
				"compare_to":1,
				"resource":"*:*_toggled_blue"
			}
		},
		"max_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/blue_max_action"
		},
		"min_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/blue_min_action"
		}
	},
	"toggled_orange":{
		"type":"origins:resource",
		"min":0,
		"max":1,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_01.png",
			"bar_index":10,
			"condition":{
				"type":"origins:resource",
				"comparison":"==",
				"compare_to":1,
				"resource":"*:*_toggled_orange"
			}
		},
		"max_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/orange_max_action"
		},
		"min_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/orange_min_action"
		}
	},
	"summon_blue":{
		"type":"origins:active_self",
		"cooldown":5,
		"key":"key.origins.primary_active",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"comparison":"==",
				"compare_to":1,
				"resource":"*:*_toggled_blue"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_toggled_blue",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_toggled_blue",
				"change":1
			}
		}
	},
	"summon_orange":{
		"type":"origins:active_self",
		"cooldown":5,
		"key":"key.origins.secondary_active",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"comparison":"==",
				"compare_to":1,
				"resource":"*:*_toggled_orange"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_toggled_orange",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_toggled_orange",
				"change":1
			}
		}
	},
	"portal":{
		"type":"origins:action_over_time",
		"interval":75,
		"entity_action":{
			"type":"origins:if_else_list",
			"actions":[
				{
					"condition":{
						"type":"origins:command",
						"comparison":">=",
						"compare_to":1,
						"command":"execute at @e[tag=Blue_Portal,limit=1,sort=nearest] as @e[tag=Blue_Portal,limit=1,sort=nearest] if entity @e[distance=..1,type=!minecraft:armor_stand,type=!minecraft:area_effect_cloud]"
					},
					"action":{
						"type":"origins:delay",
						"ticks":50,
						"action":{
							"type":"origins:execute_command",
							"command":"function origins-plus-plus:warper/blue_portal"
						}
					}
				},
				{
					"condition":{
						"type":"origins:command",
						"comparison":">=",
						"compare_to":1,
						"command":"execute at @e[tag=Orange_Portal,limit=1,sort=nearest] as @e[tag=Orange_Portal,limit=1,sort=nearest] if entity @e[distance=..1,type=!minecraft:armor_stand,type=!minecraft:area_effect_cloud]"
					},
					"action":{
						"type":"origins:delay",
						"ticks":50,
						"action":{
							"type":"origins:execute_command",
							"command":"function origins-plus-plus:warper/orange_portal"
						}
					}
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"comparison":"==",
					"compare_to":1,
					"resource":"*:*_toggled_blue"
				},
				{
					"type":"origins:resource",
					"comparison":"==",
					"compare_to":1,
					"resource":"*:*_toggled_orange"
				}
			]
		}
	},
	"particle":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/particles"
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"comparison":"==",
					"compare_to":1,
					"resource":"*:*_toggled_blue"
				},
				{
					"type":"origins:resource",
					"comparison":"==",
					"compare_to":1,
					"resource":"*:*_toggled_orange"
				}
			]
		}
	},
	"clean_up":{
		"type":"origins:action_on_callback",
		"entity_action_lost":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:warper/clean_up"
		}
	}
}