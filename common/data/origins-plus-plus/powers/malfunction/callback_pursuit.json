{
	"type":"origins:multiple",
	"pin":{
		"type":"origins:target_action_on_hit",
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:malfunction/pin"
		},
		"cooldown":5,
		"hud_render":{
			"bar_index":2
		}
	},
	"cooldown":{
		"type":"origins:resource",
		"min":0,
		"max":600,
		"hud_render":{
			"bar_index":7
		}
	},
	"callback":{
		"type":"origins:active_self",
		"key":"key.saveToolbarActivator",
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"function origins-plus-plus:malfunction/callback"
				},
				{
					"type":"origins:delay",
					"ticks":5,
					"action":{
						"type":"origins:execute_command",
						"command":"playsound minecraft:block.portal.trigger ambient @a ~ ~ ~ 2 2"
					}
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"*:*_cooldown",
					"comparison":"==",
					"compare_to":600
				},
				{
					"type":"origins:sneaking",
					"inverted":true
				}
			]
		}
	},
	"pursuit":{
		"type":"origins:active_self",
		"key":"key.saveToolbarActivator",
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"function origins-plus-plus:malfunction/pursuit"
				},
				{
					"type":"origins:delay",
					"ticks":5,
					"action":{
						"type":"origins:execute_command",
						"command":"playsound minecraft:block.portal.trigger ambient @a ~ ~ ~ 2 2"
					}
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"*:*_cooldown",
					"comparison":"==",
					"compare_to":600
				},
				{
					"type":"origins:sneaking"
				}
			]
		}
	},
	"increase_cooldown":{
		"type":"origins:action_over_time",
		"interval":20,
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_cooldown",
			"change":20
		}
	}
}