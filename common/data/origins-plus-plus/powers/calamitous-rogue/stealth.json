{
	"type":"origins:multiple",
	"toggle":{
		"type":"origins:toggle",
		"key":"key.origins.secondary_active"
	},
	"bar":{
		"type":"origins:resource",
		"min":0,
		"max":5,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/huang/resource_bar_01.png",
			"bar_index":5
		}
	},
	"bad_dmg":{
		"type":"origins:modify_damage_dealt",
		"bientity_condition":{
			"type":"origins:actor_condition",
			"condition":{
				"type":"origins:resource",
				"resource":"*:*_bar",
				"comparison":"<",
				"compare_to":5
			}
		},
		"modifier":{
			"operation":"multiply_total",
			"value":-0.5
		}
	},
	"fade":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_bar",
			"change":1
		},
		"interval":20
	},
	"break":{
		"type":"origins:self_action_on_hit",
		"entity_action":{
			"type":"origins:delay",
			"ticks":1,
			"action":{
				"type":"origins:change_resource",
				"resource":"*:*_bar",
				"change":-5
			}
		},
		"damage_condition":{
			"type":"origins:name",
			"name":"aggro",
			"inverted":true
		},
		"cooldown":1
	},
	"break2":{
		"type":"origins:self_action_when_hit",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_bar",
			"change":-5
		},
		"cooldown":1
	},
	"fade1":{
		"type":"origins:model_color",
		"alpha":0.7,
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_bar",
			"compare_to":1,
			"comparison":"=="
		}
	},
	"fade2":{
		"type":"origins:model_color",
		"alpha":0.5,
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_bar",
			"compare_to":2,
			"comparison":"=="
		}
	},
	"fade3":{
		"type":"origins:model_color",
		"alpha":0.3,
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_bar",
			"compare_to":3,
			"comparison":"=="
		}
	},
	"fade4":{
		"type":"origins:model_color",
		"alpha":0.1,
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_bar",
			"compare_to":4,
			"comparison":"=="
		}
	},
	"invisible":{
		"type":"origins:invisibility",
		"render_armor":false,
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"*:*_bar",
					"compare_to":5,
					"comparison":"=="
				},
				{
					"type":"origins:power_active",
					"power":"*:*_toggle"
				}
			]
		}
	},
	"pacify":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:execute_command",
			"command":"power grant @e[type=!minecraft:player,type=!#origins-plus-plus:untargetable,distance=..32] origins-plus-plus:calamitous-rogue/pacify"
		},
		"interval":1,
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"*:*_bar",
					"compare_to":5,
					"comparison":"=="
				},
				{
					"type":"origins:power_active",
					"power":"*:*_toggle"
				}
			]
		}
	},
	"power_gained":{
		"type":"origins:action_on_callback",
		"entity_action_gained":{
			"type":"origins:execute_command",
			"command":"function origins-plus-plus:calamitous-rogue/team"
		}
	},
	"power_lost":{
		"type":"origins:action_on_callback",
		"entity_action_lost":{
			"type":"origins:execute_command",
			"command":"team leave @s"
		}
	}
}