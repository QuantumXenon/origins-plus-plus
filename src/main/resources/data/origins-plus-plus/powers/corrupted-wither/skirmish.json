{
	"type":"origins:multiple",
	"on":{
		"type":"origins:resource",
		"min":0,
		"max":1
	},
	"timer":{
		"type":"origins:resource",
		"min":0,
		"max":10,
		"hud_render":{
			"should_render":true,
			"bar_index":3
		}
	},
	"toggle_on":{
		"type":"origins:active_self",
		"cooldown":50,
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"resource":"*:*_on",
				"comparison":"==",
				"compare_to":1
			},
			"if_action":{
				"type":"origins:and",
				"actions":[
					{
						"type":"origins:change_resource",
						"resource":"*:*_on",
						"change":-1
					},
					{
						"type":"origins:execute_command",
						"command":"playsound minecraft:block.beacon.power_select ambient @s ~ ~ ~ 10 2"
					}
				]
			},
			"else_action":{
				"type":"origins:and",
				"actions":[
					{
						"type":"origins:change_resource",
						"resource":"*:*_on",
						"change":1
					},
					{
						"type":"origins:execute_command",
						"command":"playsound minecraft:block.beacon.power_select ambient @s ~ ~ ~ 10 2"
					}
				]
			}
		}
	},
	"modify_swim_speed":{
		"type":"origins:modify_swim_speed",
		"modifier":{
			"operation":"addition",
			"value":0.03
		},
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"phasing":{
		"type":"origins:phasing",
		"blacklist":true,
		"block_condition":{
			"type":"origins:in_tag",
			"tag":"origins:unphasable"
		},
		"view_distance":15,
		"phase_down_condition":{
			"type":"origins:swimming"
		},
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"invisibility":{
		"type":"origins:invisibility",
		"render_armor":false,
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"prevent_entity_render":{
		"type":"origins:prevent_entity_render",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"shader":{
		"type":"origins:shader",
		"shader":"minecraft:shaders/post/desaturate.json",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"prevent_block_use":{
		"type":"origins:prevent_block_use",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"swimming":{
		"type":"origins:swimming",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		}
	},
	"invulnerability":{
		"type":"origins:invulnerability",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_on",
			"comparison":"==",
			"compare_to":1
		},
		"damage_condition":{
			"type":"origins:name",
			"name":"outOfWorld",
			"inverted":true
		}
	},
	"off":{
		"type":"origins:action_over_time",
		"interval":10,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:change_resource",
					"resource":"*:*_on",
					"change":-1
				},
				{
					"type":"origins:execute_command",
					"command":"playsound minecraft:block.beacon.power_select ambient @s ~ ~ ~ 10 2"
				}
			]
		},
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_timer",
			"comparison":"==",
			"compare_to":0
		}
	},
	"gen":{
		"type":"origins:action_over_time",
		"interval":20,
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"resource":"*:*_on",
				"comparison":"==",
				"compare_to":1
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_timer",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_timer",
				"change":1
			}
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:modify_resource",
			"modifier":{
				"operation":"set_total",
				"value":10
			},
			"resource":"*:*_timer"
		}
	}
}