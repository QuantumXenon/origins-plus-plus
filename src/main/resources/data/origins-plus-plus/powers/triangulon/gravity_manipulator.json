{
	"type":"origins:multiple",
	"flight":{
		"type":"origins:creative_flight",
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_timer",
			"comparison":">",
			"compare_to":1
		}
	},
	"timer":{
		"type":"origins:resource",
		"min":0,
		"max":400,
		"start_value":400,
		"hud_render":{
			"bar_index":4
		}
	},
	"countdown":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:creative_flying"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_timer",
				"change":-2
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_timer",
				"change":1
			}
		},
		"interval":2
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:modify_resource",
			"modifier":{
				"operation":"set_total",
				"value":400
			},
			"resource":"*:*_timer"
		}
	}
}