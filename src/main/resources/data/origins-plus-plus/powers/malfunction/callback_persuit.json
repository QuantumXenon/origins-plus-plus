{
  "type":"origins:multiple",
  "pin": {
      "type": "origins:target_action_on_hit",
      "entity_action": {
        "type":"origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command":"tag @e[tag=tp] remove tp"
          },
          {
            "type": "origins:execute_command",
            "command":"tag @s add tp"
          }
        ]
      },
      "cooldown": 5,
      "hud_render": {
          "should_render": true,
          "bar_index": 2
      }
  },
  "cooldown": {
    "type": "origins:resource",
    "min": 0,
    "max": 600,
    "hud_render": {
      "should_render": true,
      "bar_index": 7,
      "condition": {
        "type":"origins:sneaking"
      }
    }
  },
  "callback": {
    "type":"origins:active_self",
    "cooldown": 0,
    "hud_render": {
      "should_render": false
    },
	"key": "key.saveToolbarActivator",
    "entity_action": {
      "type":"origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command":"tp @e[tag=tp] @s"
        },
        {
          "type": "origins:execute_command",
          "command":"effect give @s minecraft:blindness 2 2 true"
        },
        {
          "type": "origins:execute_command",
          "command":"effect give @e[tag=tp] minecraft:blindness 2 2 true"
        },
        {
          "type": "origins:delay",
          "ticks": 5,
          "action": {
            "type": "origins:execute_command",
            "command":"playsound minecraft:block.portal.trigger ambient @a ~ ~ ~ 2 2"
          }
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_cooldown",
          "change": -600
        }
      ]
    },
    "condition": {
      "type":"origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_cooldown",
          "comparison": "==",
          "compare_to": 600
        },
        {
          "type":"origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "persuit": {
    "type":"origins:active_self",
    "cooldown": 0,
    "hud_render": {
      "should_render": false
    },
    "key": "key.saveToolbarActivator",
    "entity_action": {
      "type":"origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command":"tp @s @e[limit=1,tag=tp]"
        },
        {
          "type": "origins:execute_command",
          "command":"effect give @s minecraft:blindness 2 2 true"
        },
        {
          "type": "origins:execute_command",
          "command":"effect give @e[tag=tp] minecraft:blindness 2 2 true"
        },
        {
          "type": "origins:delay",
          "ticks": 5,
          "action": {
            "type": "origins:execute_command",
            "command":"playsound minecraft:block.portal.trigger ambient @a ~ ~ ~ 2 2"
          }
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_cooldown",
          "change": -600
        }
      ]
    },
    "condition": {
      "type":"origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_cooldown",
          "comparison": "==",
          "compare_to": 600
        },
        {
          "type":"origins:sneaking"
        }
      ]
    }
  },
  "increase_cooldown": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_cooldown",
      "change": 20
    }
  },
  "name": "Entity Teleportation",
  "description": "You can teleport to or teleport to you the last entity you punched. Activate without shifting to teleport it to you, and with shifting to teleport to it."
}
