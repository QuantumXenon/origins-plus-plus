            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "inverted": false,
                "comparison": "==",
                "resource": "*:*_toggle",
                "compare_to": 1
            },
            "if_action": {
                "type": "origins:change_resource",
                "resource": "*:*_toggle",
                "change": -1
            },
            "else_action": {
                "type": "origins:change_resource",
                "resource": "*:*_toggle",
                "change": 1
            }
        },
		"hud_render": {
            "should_render": false,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 1
        }
    },
