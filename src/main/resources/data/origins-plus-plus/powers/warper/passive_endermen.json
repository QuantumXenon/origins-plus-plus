{
	"type":"origins:multiple",
	"callback":{
		"type":"origins:action_on_callback",
		"entity_action_chosen":{
			"type":"origins:execute_command",
			"command":"team add passive_enderman"
		},
		"entity_action_added":{
			"type":"origins:delay",
			"ticks":1,
			"action":{
				"type":"origins:execute_command",
				"command":"team join passive_enderman @s"
			}
		},
		"entity_action_lost":{
			"type":"origins:execute_command",
			"command":"team leave @s"
		}
	},
	"join_team":{
		"type":"origins:action_over_time",
		"interval":20,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"team join passive_enderman @e[type=enderman,nbt={HurtByTimestamp:0}]"
		}
	},
	"leave_team":{
		"type":"origins:target_action_on_hit",
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"team leave @s[type=enderman]"
				},
				{
					"type":"origins:delay",
					"ticks":400,
					"action":{
						"type":"origins:execute_command",
						"command":"team join passive_enderman"
					}
				}
			]
		}
	}
}