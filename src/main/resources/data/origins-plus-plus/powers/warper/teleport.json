{
    "name": "Teleport",
    "description": "Teleport wherever you're looking within 64 blocks.",
	"type": "origins:active_self",
	"key": "key.spectatorOutlines",
    "hud_render": {
        "should_render": true
    },
    "entity_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:raycast",
                "distance": 64,
                "entity": false,
                "before_action": {
                    "type": "origins:and",
                    "actions": {
                        "type": "origins:execute_command",
                        "command": "tag @s add tping"
                    }
                },
                "command_step": 1,
                "command_along_ray": "execute positioned ~ ~ ~ if block ~ ~ ~ air run tp @e[tag=tping,sort=nearest] ~ ~ ~",
                "miss_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:add_velocity",
                            "space": "local",
                            "z": 0.1
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tag @s remove tping"
                        }
                    ]
                },
                "hit_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:add_velocity",
                            "space": "local",
                            "z": 0.1
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tag @s remove tping"
                        }
                    ]
                }
            }
        ]
	},
	"cooldown": 1200,
    "hud_render": {
        "should_render": true,
        "sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
        "bar_index": 3,
    }
}