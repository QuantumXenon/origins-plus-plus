{
    "name": "Stomach Body",
    "description": "You have increased saturation from meats (both raw and cooked) thanks to how your body breaks down the matter, and don't get hunger or poison status effects. However, you have no regeneration, and must eat to get back hearts directly.",
    "type": "origins:multiple",

    "no_hunger": {
    "type": "origins:effect_immunity",
    "effects": [
        "minecraft:hunger",
        "minecraft:poison"
    ]
},
    "no_regen":
    {
        "type": "origins:disable_regen"
    },
    "no_spint":
    {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action":
        {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition":
                    {
                        "type": "origins:food_level",
                        "inverted": false,
                        "comparison": ">",
                        "compare_to": 10
                    },
                    "if_action":
                    {
                        "type": "origins:feed",
                        "food": -1,
                        "saturation": 0
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition":
                    {
                        "type": "origins:food_level",
                        "inverted": false,
                        "comparison": "<",
                        "compare_to": 10
                    },
                    "if_action":
                    {
                        "type": "origins:feed",
                        "food": 1,
                        "saturation": 0
                    }
                }
            ]
        }
    },
    "stop_when_full":
    {
        "type": "origins:prevent_item_use",
        "item_condition":
        {
            "type": "origins:and",
            "inverted": false,
            "conditions": [
                {
                    "type": "origins:food",
                    "inverted": false
                },
                {
                    "type": "origins:ingredient",
                    "inverted": true,
                    "ingredient":
                    {
                        "item": "chorus_fruit"
                    }
                },
                {
                    "type": "origins:ingredient",
                    "inverted": true,
                    "ingredient":
                    {
                        "item": "golden_apple"
                    }
                },
                {
                    "type": "origins:ingredient",
                    "inverted": true,
                    "ingredient":
                    {
                        "item": "enchanted_golden_apple"
                    }
                },
                {
                    "type": "origins:ingredient",
                    "inverted": true,
                    "ingredient":
                    {
                        "item": "honey_bottle"
                    }
                },
                {
                    "type": "origins:ingredient",
                    "inverted": true,
                    "ingredient":
                    {
                        "item": "suspicious_stew"
                    }
                }
            ]
        },
        "condition":
        {
            "type": "origins:relative_health",
            "inverted": false,
            "comparison": "==",
            "compare_to": 1
        }
    },
