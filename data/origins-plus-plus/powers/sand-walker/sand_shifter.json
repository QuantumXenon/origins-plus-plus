{
	"type":"origins:multiple",
	"togglephase":{
		"type":"origins:toggle"
	},
	"phase":{
		"type":"origins:phasing",
		"blacklist":true,
		"render_type":"blindness",
		"view_distance":10,
		"block_condition":{
			"type":"origins:in_tag",
			"tag":"origins-plus-plus:burrow_blocks",
			"inverted":true
		},
		"phase_down_condition":{
			"type":"origins:sneaking"
		},
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_togglephase"
		}
	},
	"invuln":{
		"type":"origins:invulnerability",
		"damage_condition":{
			"type":"origins:name",
			"name":"fall"
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:sneaking"
				},
				{
					"type":"origins:power_active",
					"power":"*:*_togglephase"
				}
			]
		}
	},
	"ignorewater":{
		"type":"origins:ignore_water",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_togglephase"
		}
	},
	"ascend":{
		"type":"origins:active_self",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:and",
				"conditions":[
					{
						"type":"origins:in_block",
						"block_condition":{
							"type":"origins:height",
							"comparison":"<=",
							"compare_to":128
						}
					},
					{
						"type":"origins:resource",
						"resource":"*:*_resource",
						"comparison":"==",
						"compare_to":1
					}
				]
			},
			"else_action":{
				"type":"origins:damage",
				"amount":0,
				"source":{
					"name":"null",
					"bypasses_armor":true
				}
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_up_resource",
				"change":4
			}
		},
		"key":"key.jump"
	},
	"descend":{
		"type":"origins:active_self",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_down_resource",
			"change":4
		},
		"key":"key.sneak"
	},
	"float":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"resource":"*:*_up_resource",
				"comparison":"!=",
				"compare_to":0
			},
			"if_action":{
				"type":"origins:apply_effect",
				"effect":{
					"effect":"minecraft:levitation",
					"duration":2,
					"amplifier":1,
					"show_particles":false,
					"show_icon":false
				}
			},
			"else_action":{
				"type":"origins:if_else",
				"condition":{
					"type":"origins:resource",
					"resource":"*:*_down_resource",
					"comparison":"!=",
					"compare_to":0
				},
				"if_action":{
					"type":"origins:if_else",
					"condition":{
						"type":"origins:status_effect",
						"effect":"minecraft:levitation"
					},
					"if_action":{
						"type":"origins:clear_effect",
						"effect":"minecraft:levitation"
					},
					"else_action":{
						"type":"origins:apply_effect",
						"effect":{
							"effect":"minecraft:levitation",
							"duration":2,
							"amplifier":-5,
							"show_particles":false,
							"show_icon":false
						}
					}
				},
				"else_action":{
					"type":"origins:apply_effect",
					"effect":{
						"effect":"minecraft:levitation",
						"duration":2,
						"amplifier":-1,
						"show_particles":false,
						"show_icon":false
					}
				}
			}
		},
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_resource",
			"comparison":"==",
			"compare_to":1
		},
		"interval":1
	},
	"down_resource":{
		"type":"origins:resource",
		"min":0,
		"max":2
	},
	"up_resource":{
		"type":"origins:resource",
		"min":0,
		"max":2
	},
	"resource":{
		"type":"origins:resource",
		"min":0,
		"max":1
	},
	"toggle":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:and",
				"conditions":[
					{
						"type":"origins:power_active",
						"power":"*:*_togglephase"
					},
					{
						"type":"origins:in_block",
						"block_condition":{
							"type":"origins:movement_blocking"
						}
					}
				]
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":-1
			}
		},
		"interval":1
	},
	"drain1":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_up_resource",
			"change":-1
		},
		"interval":1
	},
	"drain2":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_down_resource",
			"change":-1
		},
		"interval":1
	}
}