{
	"type":"origins:multiple",
	"resource":{
		"type":"origins:resource",
		"min":0,
		"max":1,
		"hud_render":{
			"should_render":false
		}
	},
	"switch":{
		"type":"origins:active_self",
		"key":"key.origins.primary_active",
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:resource",
				"resource":"*:*_resource",
				"comparison":"==",
				"compare_to":0
			},
			"if_action":{
				"type":"origins:and",
				"actions":[
					{
						"type":"origins:execute_command",
						"command":"scale set pehkui:height 0.25"
					},
					{
						"type":"origins:execute_command",
						"command":"scale set pehkui:width 0.25"
					},
					{
						"type":"origins:change_resource",
						"resource":"*:*_resource",
						"change":1,
						"operation":"set"
					},
					{
						"type":"origins:execute_command",
						"command":"effect give @p minecraft:slowness 999999 2 true"
					}
				]
			},
			"else_action":{
				"type":"origins:and",
				"actions":[
					{
						"type":"origins:execute_command",
						"command":"scale reset"
					},
					{
						"type":"origins:change_resource",
						"resource":"*:*_resource",
						"change":0,
						"operation":"set"
					},
					{
						"type":"origins:execute_command",
						"command":"effect clear @p minecraft:slowness"
					}
				]
			}
		}
	},
	"power_lost":{
		"type":"origins:action_on_callback",
		"entity_action_lost":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"scale reset"
				},
				{
					"type":"origins:change_resource",
					"resource":"*:*_resource",
					"change":0,
					"operation":"set"
				},
				{
					"type":"origins:execute_command",
					"command":"effect clear @p minecraft:slowness"
				}
			]
		}
	}
}