{
	"type":"origins:multiple",
	"cooldown":{
		"type":"origins:resource",
		"min":0,
		"max":5,
		"hud_render":{
			"should_render":true,
			"bar_index":3
		}
	},
	"tick":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:change_resource",
			"resource":"*:*_cooldown",
			"change":-1
		},
		"interval":40
	},
	"if_clone":{
		"type":"origins:active_self",
		"key":"key.origins.secondary_active",
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"function origins-plus-plus:enigma/if_clone"
				},
				{
					"type":"origins:if_else",
					"condition":{
						"type":"origins:scoreboard",
						"objective":"Enigma_Clone",
						"comparison":">=",
						"compare_to":1
					},
					"if_action":{
						"type":"origins:and",
						"actions":[
							{
								"type":"origins:clear_effect",
								"effect":"minecraft:invisibility"
							},
							{
								"type":"origins:execute_command",
								"command":"execute at @s as @e[type=minecraft:zombie,tag=Enigma_Clone] if score @s UUID1 = @a[distance=..0.001,sort=nearest,limit=1] UUID1 if score @s UUID2 = @a[distance=..0.001,sort=nearest,limit=1] UUID2 if score @s UUID3 = @a[distance=..0.001,sort=nearest,limit=1] UUID3 if score @s UUID4 = @a[distance=..0.001,sort=nearest,limit=1] UUID4 run tp @s 0 -999 -0"
							}
						]
					},
					"else_action":{
						"type":"origins:if_else",
						"condition":{
							"type":"origins:resource",
							"resource":"*:*_cooldown",
							"comparison":"==",
							"compare_to":0
						},
						"if_action":{
							"type":"origins:execute_command",
							"command":"function origins-plus-plus:enigma/clone"
						}
					}
				}
			]
		},
		"condition":{
			"type":"origins:power_active",
			"power":"origins-plus-plus:enigma/phase_toggle",
			"inverted":true
		}
	},
	"no_invisibility":{
		"type":"origins:self_action_when_hit",
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:clear_effect",
					"effect":"minecraft:invisibility"
				},
				{
					"type":"origins:execute_command",
					"command":"execute at @s as @e[type=minecraft:zombie,tag=Enigma_Clone] if score @s UUID1 = @a[distance=..0.001,sort=nearest,limit=1] UUID1 if score @s UUID2 = @a[distance=..0.001,sort=nearest,limit=1] UUID2 if score @s UUID3 = @a[distance=..0.001,sort=nearest,limit=1] UUID3 if score @s UUID4 = @a[distance=..0.001,sort=nearest,limit=1] UUID4 run tp @s 0 -999 -0"
				}
			]
		},
		"cooldown":1
	},
	"get_damage":{
		"type":"origins:action_over_time",
		"rising_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:damage",
					"amount":6,
					"source":{
						"name":"magic",
						"bypasses_armor":true
					}
				},
				{
					"type":"origins:clear_effect",
					"effect":"minecraft:invisibility"
				},
				{
					"type":"origins:execute_command",
					"command":"tag @s remove Enigma_Damage"
				}
			]
		},
		"interval":1,
		"condition":{
			"type":"origins:nbt",
			"nbt":"{Tags:['Enigma_Damage']}"
		}
	}
}